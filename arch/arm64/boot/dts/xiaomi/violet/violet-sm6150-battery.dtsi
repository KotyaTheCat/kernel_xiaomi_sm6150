	#include "battery/LC_M6300_Sunwoda4000mAh_3632066_FG.dtsi"
};

&smb1355 {
	status = "disabled";
};

&smb1355_charger {
	status = "disabled";
};

&smb1390 {
	status = "disabled";
};

